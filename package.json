{
  "name": "react-oc-remote-edit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.42",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/websocket": "^1.0.0",
    "autoprefixer": "^9.8.0",
    "cross-env": "^7.0.2",
    "customize-cra": "^1.0.0",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "postcss-cli": "^7.1.1",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-dom": "^16.13.1",
    "react-monaco-editor": "^0.36.0",
    "react-scripts": "3.4.1",
    "react-use-websocket": "^2.0.1",
    "react-websocket": "^2.1.0",
    "tailwindcss": "^1.4.6",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5",
    "websocket": "^1.0.31"
  },
  "scripts": {
    "start2": "run-p watch:css start:server start:app",
    "start": "npm-run-all -l -p start:server start:app watch:css",
    "start:app": "react-app-rewired start",
    "start:server": "cross-env-shell PORT=9898 \"cd ./server && ts-node-dev --project ./tsconfig.json ./server.ts && wait-on tcp:9898\"",
    "build": "run-s build:css build:app",
    "build:app": "react-app-rewired build",
    "build:css": "postcss src/styles/index.css -o src/index.css",
    "test": "react-app-rewired test",
    "watch:css": "onchange src/styles/**/* -- npm run build:css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
